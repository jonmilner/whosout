<?xml version="1.0" encoding="utf-8"?>
<configuration>
 <system.webServer>
  <httpErrors>
    <remove statusCode="404" subStatusCode="-1" />
    <error statusCode="404" prefixLanguageFilePath="" path="/index.html" responseMode="ExecuteURL" />
  </httpErrors>
  <httpProtocol>
    <customHeaders>
      <add name="Access-Control-Allow-Origin" value="*" />
    </customHeaders>
  </httpProtocol>
 </system.webServer>
</configuration>
